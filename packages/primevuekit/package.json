{
    "name": "primevuekit",
    "version": "1.0.0",
    "author": "Danny AI Studio",
    "description": "A collection of specialized Vue components extending PrimeVue's functionality for enterprise applications.",
    "main": "./src/index.js",
    "module": "./src/index.js",
    "types": "./src/index.d.ts",
    "exports": {
        ".": {
            "import": "./src/index.js",
            "require": "./src/index.js",
            "types": "./src/index.d.ts"
        },
        "./inputdatemask": {
            "import": "./src/inputdatemask/index.js",
            "require": "./src/inputdatemask/index.js",
            "types": "./src/inputdatemask/index.d.ts"
        }
    },
    "scripts": {
        "build": "NODE_ENV=production INPUT_DIR=src/ OUTPUT_DIR=dist/ pnpm run build:package",
        "build:package": "pnpm run build:prebuild && rollup -c && pnpm run build:postbuild",
        "build:prebuild": "node ./scripts/prebuild.mjs",
        "build:postbuild": "node ./scripts/postbuild.mjs",
        "dev:link": "pnpm link --global && npm link"
    },
    "keywords": [
        "primevue",
        "vue",
        "vue.js",
        "vue3",
        "ui library",
        "component library"
    ],
    "license": "MIT",
    "packageManager": "pnpm@10.12.1",
    "dependencies": {
        "date-fns": "^4.1.0",
        "primevue": "catalog:",
        "vue": "^3.5.17"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hugcanada/PrimeVueKit"
    },
    "bugs": {
        "url": "https://github.com/hugcanada/PrimeVueKit/issues"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.28.0",
        "@babel/preset-env": "^7.28.0",
        "@babel/preset-typescript": "^7.27.1",
        "@rollup/plugin-alias": "^5.1.1",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/node": "^24.0.13",
        "@vitejs/plugin-vue": "^6.0.0",
        "fs-extra": "^11.3.0",
        "rollup": "^4.44.2",
        "typescript": "^5.8.3"
    }
}
